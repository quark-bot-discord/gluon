{
  "name": "gluon",
  "version": "2.0.0",
  "description": "Discord API library for Quark",
  "keywords": [
    "discord",
    "api",
    "library",
    "quark"
  ],
  "exports": {
    ".": {
      "default": "./index.js"
    },
    "./constants": {
      "default": "./src/constants.js"
    },
    "./util": {
      "default": "./src/util.js"
    },
    "./structures": {
      "default": "./src/structures.js"
    },
    "./structures/compiled": {
      "default": "./src/compiledStructures.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "mocha \"test/**/*.js\" --file test/setup.js",
    "example": "node ./example/bot.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quark-bot-discord/gluon.git"
  },
  "type": "module",
  "author": "Starman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quark-bot-discord/gluon/issues",
    "email": "bugs.gluon@quark.bot"
  },
  "homepage": "https://gluon.quark.bot",
  "dependencies": {
    "bufferutil": "^4.0.8",
    "chalk": "^5.3.0",
    "erlpack": "github:quark-bot-discord/erlpack",
    "fastq": "^1.17.1",
    "form-data": "^4.0.0",
    "hash.js": "^1.1.7",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "chai-spies": "^1.1.0",
    "clean-jsdoc-theme": "^4.3.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "mocha": "^10.6.0",
    "msw": "^2.3.4",
    "prettier": "3.3.3",
    "sinon": "^18.0.0",
    "sinon-chai": "^4.0.0"
  },
  "script": {
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
