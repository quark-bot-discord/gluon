{
  "name": "gluon",
  "version": "2.1.1",
  "description": "Discord API library for Quark",
  "keywords": [
    "discord",
    "api",
    "library",
    "quark"
  ],
  "exports": {
    "./constants": {
      "types": "./dist/src/constants.d.ts",
      "default": "./dist/src/constants.js"
    },
    "./util": {
      "types": "./dist/src/util.d.ts",
      "default": "./dist/src/util.js"
    },
    "./structures": {
      "types": "./dist/src/structures.d.ts",
      "default": "./dist/src/structures.js"
    },
    "./testData": {
      "types": "./dist/src/testData.d.ts",
      "default": "./dist/src/testData.js"
    },
    "./discord-api-types": {
      "types": "./dist/typings/discord.d.ts",
      "default": "./dist/typings/discord.js"
    },
    "./@types": {
      "types": "./typings/index.d.ts"
    },
    "./@errors": {
      "types": "./dist/typings/errors.d.ts",
      "default": "./dist/typings/errors.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "npm run build && mocha \"dist/**/*.spec.js\" --file setup.spec.js --exit",
    "coverage": "npm run build && c8 --reporter=text mocha \"dist/**/*.spec.js\" --file setup.spec.js --exit",
    "example": "node ./example/bot.js",
    "prepare": "husky",
    "build": "rm -rf dist/ && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quark-bot-discord/gluon.git"
  },
  "imports": {
    "#typings/*": "./typings/*",
    "#src/*": "./src/*",
    "#managers/*": "./src/managers/*",
    "#structures/*": "./src/structures/*",
    "#util/*": "./src/util/*"
  },
  "type": "module",
  "author": "Starman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quark-bot-discord/gluon/issues",
    "email": "bugs.gluon@quark.bot"
  },
  "homepage": "https://gluon.quark.bot",
  "dependencies": {
    "bufferutil": "^4.0.8",
    "chalk": "^5.4.1",
    "discord-api-types": "^0.37.119",
    "erlpack": "github:quark-bot-discord/erlpack",
    "form-data": "^4.0.0",
    "hash.js": "^1.1.7",
    "ioredis": "^5.5.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "tiny-typed-emitter": "^2.1.0",
    "ws": "^8.18.0",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/chai": "^5.2.0",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.5",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "c8": "^10.1.3",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.1",
    "chai-spies": "^1.1.0",
    "eslint": "^9.21.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "mocha": "^10.6.0",
    "msw": "^2.3.4",
    "prettier": "3.3.3",
    "sinon": "^18.0.0",
    "sinon-chai": "^4.0.0",
    "source-map-support": "^0.5.21",
    "testdouble": "^3.20.2",
    "ts-node": "^10.9.2",
    "typedoc-material-theme": "^1.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.25.0"
  },
  "script": {
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
